{
    "name": "t3n/neos-debug",
    "description": "Helper package to visualize debug inforamations for neos including fusion content cache",
    "license": "MIT",
    "type": "neos-package",
    "require": {
        "php": ">=8.2",
        "neos/neos": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "t3n\\Neos\\Debug\\": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "t3n.Neos.Debug",
            "loading-order": {
                "after": [ "neos/neos" ]
            }
        }
    }
}
